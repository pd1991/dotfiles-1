[user]
  name = Pradyumna
  email = pradyumna2905@gmail.com
[alias]
  co = checkout
  cleanup = !git remote prune origin && git gc && git clean -df && git stash clear
[color]
  diff = auto
  status = auto
  branch = auto

[color "diff"]
  meta = yellow

[core]
  excludesfile = ~/.gitignore
  editor = vim --noplugin
[apply]
  whitespace = nowarn

[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
	d = difftool
[help]
  autocorrect = 1
[branch]
  autosetupmerge = always
  autosetuprebase = always
[push]
  default = current
[rerere]
  enabled = 1
[github]
  user = pradyumna2905
[merge]
  ff = only
[fetch]
  prune = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[mergetool]
	keepBackup = false
